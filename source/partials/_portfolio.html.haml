
.row
  %a{name: "portfolio"}
  .row-padding
    .row.small-up-2.medium-up-3.large-up-4
      -data.projects.each do |project|
        .column
          %a{href: "#{project.link}"}
            %img= image_tag project[:img], class: 'thumb'
            %h4= project[:name]
            %h7= project[:description]
